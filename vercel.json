{
  "builds": [
    {
      "src": "api/chatbot.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/vite"
    }
  ],
  "routes": [
    {
      "src": "/api/chatbot",
      "dest": "api/chatbot.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}